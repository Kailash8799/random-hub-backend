{
  "compilerOptions": {
     "lib": [
        "es6"
     ],
     "target": "ES2016",
     "module": "CommonJS",
     "strict": false,
   //   "moduleResolution": "node",
     "outDir": "dist",
     "resolveJsonModule": true,
